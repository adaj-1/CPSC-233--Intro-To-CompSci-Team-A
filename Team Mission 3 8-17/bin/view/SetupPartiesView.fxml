<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane prefHeight="203.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SetupPartiesViewController">
   <children>
      <VBox prefHeight="199.0" prefWidth="399.0">
         <children>
            <HBox prefHeight="50.0" prefWidth="399.0">
               <children>
                  <Label prefHeight="50.0" prefWidth="125.0" text="Select party to setup: " />
                  <ChoiceBox fx:id="partyDropDown" minWidth="-Infinity" prefHeight="25.0" prefWidth="140.0">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </ChoiceBox>
               </children>
               <padding>
                  <Insets left="50.0" />
               </padding>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="285.0">
               <children>
                  <Label prefHeight="50.0" prefWidth="178.0" text="Party Name: ">
                     <padding>
                        <Insets left="95.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="PartyNameTextBox" prefHeight="25.0" prefWidth="140.0">
                     <HBox.margin>
                        <Insets top="15.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="285.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#clear" prefHeight="25.0" prefWidth="70.0" text="Clear">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#PartyInfo" prefHeight="25.0" prefWidth="100.0" text="Set Party Info">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#SubmitPartyInfo" prefHeight="25.0" prefWidth="125.0" text="Submit All Party Info">
                     <HBox.margin>
                        <Insets left="30.0" right="25.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</FlowPane>
